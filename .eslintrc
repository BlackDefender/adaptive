{
    "extends": "airbnb-base",
    "env": {
        "browser": true
    },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "indent": ["error", 4],
        "no-param-reassign": ["error", {
            "props": false
        }]
    },
    "globals": {
        "$": "readonly",
        "jQuery": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 13
    }
}
